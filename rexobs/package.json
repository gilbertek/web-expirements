{
  "name": "react-rxjs",
  "version": "1.0.0",
  "description": "Demo rxjs",
  "main": "src/index.js",
  "author": "Gilbert Sewovoe-Ekoue",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel dev lint:watch",
    "dev": "webpack-dev-server -d --env.platform=web --env.environment=development --progress --color --open",
    "lint": "eslint . && yarn run prettier",
    "lint:watch": "node_modules/eslint-watch/bin/esw -w src test webpack.config.*",
    "lint:fix": "yarn run lint -- --fix",
    "test": "mocha",
    "test:watch": "yarn test -- --watch",
    "precommit": "lint-staged",
    "prettier": "prettier --write ./src/**/*.js ./src/js/**/*.js",
    "dist": "webpack -p",
    "build": "webpack -p --env.environment=production --env.platform=web --progress"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-logger": "^1.1.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.6.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.1",
    "jasmine": "^2.8.0",
    "jsdom": "^11.2.0",
    "mocha": "^4.0.1",
    "nock": "^9.0.14",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.10",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.1.2",
    "prettier": "^1.6.1",
    "react-addons-test-utils": "~15.6.0",
    "react-hot-loader": "next",
    "react-test-renderer": "^16.0.0",
    "react-test-utils": "^0.0.1",
    "redux-mock-store": "^1.2.3",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
