{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Gilbert Ekoue",
  "scripts": {
    "start": "npm-run-all --parallel dev test:watch",
    "dev": "webpack-dev-server --mode development --color --open",
    "dashboard": "webpack-dashboard -- webpack-dev-server --mode development --color --open",
    "lint": "eslint .",
    "lint:fix": "yarn run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "build": "webpack --mode production --env.platform=web --progress",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettier": "prettier **/*.js --write"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "bulma": "^0.7.1",
    "isemail": "^3.1.2",
    "moment": "^2.18.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-aria-modal": "^2.6.0",
    "react-autosuggest": "^9.3.1",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.0",
    "jsdom": "^16.5.0",
    "lodash-webpack-plugin": "^0.11.5",
    "nock": "^9.0.13",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.12.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.4.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.10",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.4",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
