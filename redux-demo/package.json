{
	"name": "redux-demo",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Gilbert Ekoue",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all --parallel dev lint:watch test:watch",
		"dev": "webpack-dev-server -d --env.platform=browser --env.environment=development --progress --color --open",
		"compile": "webpack-dev-server --color --open",
		"dashboard": "webpack-dashboard -- webpack-dev-server --color --open",
		"lint": "eslint . && yarn run prettier",
		"lint:watch": "node_modules/eslint-watch/bin/esw -w src test webpack.config.*",
		"lint:fix": "yarn run lint -- --fix",
		"test": "mocha",
		"test:watch": "yarn test -- --watch",
		"precommit": "lint-staged",
    "prettier": "prettier --write ./src/**/*.js ./src/js/**/*.js",
		"dist": "webpack -p",
		"build": "webpack -p --env.environment=production --env.platform=browser --progress"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"prop-types": "^15.5.10",
		"react": "^15.6.0",
		"react-dom": "^15.6.0",
		"react-redux": "^5.0.5",
		"react-router-dom": "^4.1.1",
		"react-router-redux": "^4.0.8",
		"redux": "^3.7.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"toastr": "^2.1.2"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.1",
		"babel-core": "^6.25.0",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^7.1.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.24.1",
		"css-loader": "^0.28.4",
		"enzyme": "^2.9.0",
		"eslint": "^4.0.0",
		"eslint-plugin-babel": "^4.1.1",
		"eslint-plugin-html": "^3.0.0",
		"eslint-plugin-import": "^2.5.0",
		"eslint-plugin-promise": "^3.5.0",
		"eslint-plugin-react": "^7.1.0",
		"eslint-watch": "^3.1.0",
		"expect": "^1.20.2",
		"extract-text-webpack-plugin": "^2.1.2",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.28.0",
		"jsdom": "^16.5.0",
		"mocha": "^3.4.2",
		"nock": "^9.0.13",
		"npm-run-all": "^4.0.2",
		"postcss": "^6.0.2",
		"postcss-cssnext": "^2.11.0",
		"postcss-import": "^10.0.0",
		"postcss-loader": "^2.0.6",
		"postcss-url": "^7.0.0",
		"prettier": "^1.4.4",
		"react-addons-test-utils": "^15.5.1",
		"react-hot-loader": "next",
		"react-test-renderer": "^15.5.4",
		"react-test-utils": "^0.0.1",
		"redux-mock-store": "^1.2.3",
		"sinon": "^2.3.5",
		"style-loader": "^0.18.2",
		"url-loader": "^0.5.9",
		"webpack": "^3.0.0",
		"webpack-dashboard": "^0.4.0",
		"webpack-dev-server": "^2.5.0"
	},
	"optionalDependencies": {
		"fsevents": "*"
	}
}
